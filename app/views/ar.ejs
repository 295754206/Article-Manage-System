

<div>
    <% data.list.map(function(item,index){ %>
        <div>
            <span><%= index+1 %></span>
            <span><%= item.username %></span>
            <span><a href="/detail?id=<%= item.id %>"><%= item.title %></a></span>
            <span><%= item.time %></span>
            <span>
                <a href="/write?id=<%=item.id%>&page=<%= data.cur %>">编辑</a>
                <a href="/article/delete?id=<%= item.id %>&page=<%= data.cur %>">删除</a>
            </span>
        </div>
    <% }) %>
</div>

<div>
    <% for(let i = 1;i <= data.total; i++ ){ %>
        <a href="/?page=<%= i %>"><%= i %></a>
    <% } %>
</div>

